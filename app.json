{
  "expo": {
    "name": "point-of-sales",
    "slug": "point-of-sales",
    "version": "1.0.0",
    "orientation": "landscape",
    "scheme": "point-of-sales",
    "icon": "./assets/logo.jpg",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.jpg",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.imankamal.pointofsales"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-sqlite"
    ],
    "extra": {
      "eas": {
        "projectId": "ae208c27-f687-4df8-ad3d-c9da90f0786b"
      }
    },
    "owner": "imankamal"
  }
}
